<launch>
	<param name="robot_description" command="$(find xacro)/xacro --inorder '$(find manipulator)/urdf/manipulator.xacro'" />
	<!-- Comibe joint values -->
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
	<!-- Show in Rviz -->
	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find manipulator)/launch/config.rviz"/>
	<!-- Send joint values -->
	<node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" >
		<param name="use_gui" value="True" />
	</node>
	<node name="joint_interface_node" pkg="joint_interface" type="joint_interface_node" />
</launch>